{
    "name": "wireui/phosphoricons",
    "description": "The phosphoricons made for laravel blade by WireUI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "livewire icons components",
        "livewire icons",
        "wireui",
        "wireui phosphoricons",
        "laravel components",
        "blade icons",
        "blade components",
        "blade phosphoricons"
    ],
    "authors": [
        {
            "name": "PH7-Jack",
            "email": "pedrolivertwd@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.19"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.21",
        "mockery/mockery": "^1.5"
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "phpcs": "./vendor/bin/php-cs-fixer fix"
    },
    "autoload": {
        "psr-4": {
            "WireUi\\PhosphorIcons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\PhosphorIcons\\PhosphorIconsServiceProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
